
<tr>
    <td><%= holiday.employee.name %></td>
    <td><%= holiday.start_date %></td>
    <td><%= holiday.finish_date %></td>
    <td><% if current_user?(holiday.user) %>
          <%= link_to '<span style="color: #DC003B;" class="glyphicon glyphicon-remove" aria-hidden="true"></span> DELETE'.html_safe, holiday, method: :delete,
                                           data: { confirm: "You sure?" } %>
        <% end %></td>
    <td><% if current_user?(holiday.user) %>
      <%= link_to('<span style="color: #6501FF;" class="glyphicon glyphicon-pencil" aria-hidden="true"></span> EDIT'.html_safe, edit_holiday_path(holiday.id)) %>

        <% end %></td>    
 
  </tr>
